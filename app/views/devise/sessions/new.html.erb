<h2>Log in</h2>

<%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="form-inputs">
    <div style="margin-top: 30px;">
        <%= content_tag(:div, "이메일", class: "") %>
        <%= f.email_field :email, id: "form_1", autofocus: true, autocomplete: "email", placeholder: "이메일", class: "" %>
    </div>
    <div style="margin-top: 10px;">
    	<div class="label">암호</div>
		<%= f.password_field :password, class: "devise_noborder", id: "form_2", autofocus: true, autocomplete: "off", placeholder: " ", value: "" %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Log in" %>
  </div>
<% end %>

